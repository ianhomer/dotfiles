#!/usr/bin/env bash

#
# Install node packages
#

#
# git-open : Open git file in browser from command line
#

npm list --global --depth=0

npm install --global cspell
npm install --global cspell-dict-lorem-ipsum
cspell-dict-lorem-ipsum-link
npm install --global cypress
npm install --global git-open
npm install --global neovim
npm install --global npm-check-updates
npm install --global serverless

echo "... Installed node packages"
