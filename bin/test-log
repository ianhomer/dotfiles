#!/usr/bin/env bash

#
# Test log functions
#

_DIR="$(cd "$(dirname "${BASH_SOURCE[0]}")" >/dev/null 2>&1 && pwd)"
. ${_DIR}/functions/log.sh

_ message
o_o "a log message"
o_o another log message
_ "info with multiple words"
_ OK status
_ error with $(bold message)
_ skip this that
o_o box this that
o_o box "a long message, a long message, a long message" that
o_box this "a long message, a long message, a long message"
trim 15 "a long message, a long message"
trim 15 "short"

# o_palette
