#!/usr/bin/env bash

#
# Install jenv versions
#

should-run $0 || exit

# Refresh / rehash jenv vonfiguration

jenv rehash 2>/dev/null
jenv refresh-plugins

set -e JAVA
for jdk in /Library/Java/JavaVirtualMachines/*/Contents/Home ; do
  echo "JDK installed : $jdk"
  $jdk/bin/java -version
  jenv add $jdk | grep -v "skip"
done

echo "JDKs registered with jenv"
jenv versions

should-run -fu $0

