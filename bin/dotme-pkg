#!/usr/bin/env bash

#
# Install pkg packages
#

should-run -m 7 $0 || exit

pkg install ctags
pkg install fasd
pkg install fish
pkg install fzf
pkg install git
pkg install keybase
pkg install neovim
pkg install openssh
pkg install silversearcher-ag
pkg install stow
pkg install termux-api
pkg install zsh

should-run -fu $0
