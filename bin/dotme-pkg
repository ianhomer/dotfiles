#!/usr/bin/env bash

#
# Install pkg packages
#

should-run -m 7 $0 || exit 0

pkg install -y ctags
pkg install -y fasd
pkg install -y fish
pkg install -y fzf
pkg install -y git
pkg install -y neovim
pkg install -y openssh
pkg install -y silversearcher-ag
pkg install -y stow
pkg install -y termux-api
pkg install -y zsh

should-run -fu $0
