{
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "azure.ppe": null,
  "azureFunctions.requestTimeout": 120,
  "editor.accessibilitySupport": "off",
  "editor.minimap.enabled": false,
  "files.autoSave": "afterDelay",
  "terminal.integrated.tabs.location": "left",
  "workbench.colorTheme": "Kanagawa",
  "workbench.editor.enablePreview": false,
  "terminal.integrated.env.osx": {
    "FIG_NEW_SESSION": "1"
  },
  "python.diagnostics.sourceMapsEnabled": true,
  "python.testing.pytestEnabled": true,
  "jest.autoRun": {
  
  },
  "[python]": {
    "editor.formatOnType": true
  },
  "CodeGPT.apiKey": "Ollama",
  "CodeGPT.Autocomplete.enabled": true,
  "CodeGPT.Autocomplete.provider": "Ollama - deepseek-coder:base",
  "CodeGPT.Autocomplete.suggestionDelay": 300,
  "CodeGPT.Autocomplete.maxTokens": 800,
}